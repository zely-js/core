{
  "name": "zely",
  "version": "1.1.1",
  "types": "./types/index.d.ts",
  "main": "./dist/index.js",
  "homepage": "https://zely.netlify.app/",
  "repository": "https://github.com/zely-js/core",
  "description": "a Backend Framework for Node.js",
  "bin": {
    "zely": "./dist/bin.js"
  },
  "files": [
    "dist",
    "types",
    "assets"
  ],
  "keywords": [
    "node",
    "backend",
    "back-end",
    "server",
    "routing"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./types/index.d.ts"
    },
    "./config": {
      "require": "./dist/config.js",
      "types": "./types/export-config.d.ts"
    },
    "./server": {
      "require": "./dist/server.js",
      "types": "./types/export-server.d.ts"
    },
    "./methods": {
      "require": "./dist/methods.js",
      "types": "./types/export-methods.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "config": [
        "types/export-config.d.ts"
      ],
      "server": [
        "types/export-server.d.ts"
      ],
      "methods": [
        "types/export-methods.d.ts"
      ]
    }
  },
  "dependencies": {
    "animaux": "^0.0.4",
    "chokidar": "^3.5.3",
    "colors": "^1.4.0",
    "esbuild": "^0.18.17",
    "esbuild-node-externals": "^1.8.0",
    "mime-types": "^2.1.35",
    "osik": "^0.0.5",
    "source-map": "^0.7.4",
    "teeti": "^0.0.1"
  },
  "devDependencies": {
    "@zely/plugin-cors": "*"
  }
}
