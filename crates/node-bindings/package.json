{
  "name": "@zely-js/node-bindings",
  "version": "0.0.1",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "zely",
    "triples": {}
  },
  "license": "MIT",
  "engines": {
    "node": ">= 10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --cargo-cwd ../.. --cargo-name zely_node_binding",
    "build:release": "napi build --platform --release --cargo-cwd ../.. --cargo-name zely_node_binding",
    "prepublishOnly": "napi prepublish -t lerna --skip-gh-release",
    "universal": "napi universal",
    "version": "napi version"
  },
  "optionalDependencies": {
    "@zely-js/node-bindings-darwin-x64": "workspace:*",
    "@zely-js/node-bindings-linux-x64-gnu": "workspace:*",
    "@zely-js/node-bindings-win32-x64-msvc": "workspace:*"
  }
}
